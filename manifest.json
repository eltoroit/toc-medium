{
  "name": "ET-TOC Medium",
  "version": "1.0",
  "description": "My Fork From https://github.com/vdeantoni/toc-medium = Table Of Contents generator for Medium stories",
  "manifest_version": 3,
  "author": "Vinicius De Antoni",
  "short_name": "et-toc-medium",
  "permissions": [],
  "background": {
    "service_worker": "service_worker.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["https://medium.com/*"],
      "css": ["tooltip.css"],
      "js": ["tooltip.js"]
    }
  ],
  "icons": {
    "16": "images/toc-medium16.png",
    "32": "images/toc-medium32.png",
    "48": "images/toc-medium48.png",
    "128": "images/toc-medium128.png"
  }
}
